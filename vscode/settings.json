{
  "cSpell.language": "en,en-GB,en-US,ru",
  "cSpell.showStatus": false,
  "cSpell.diagnosticLevel": "Hint",
  "debug.allowBreakpointsEverywhere": true,
  "editor.accessibilitySupport": "off",
  "editor.codeLens": true,
  "editor.colorDecorators": true,
  "editor.dragAndDrop": false,
  "editor.folding": false,
  "editor.showFoldingControls": "mouseover",
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 17,
  "editor.glyphMargin": true,
  "editor.minimap.enabled": true,
  "editor.renderWhitespace": "selection",
  "editor.rulers": [
    88
  ],
  "editor.scrollBeyondLastLine": false,
  "editor.snippetSuggestions": "none",
  "editor.cursorSurroundingLines": 3,
  "editor.useTabStops": true,
  "editor.formatOnPaste": true,
  "editor.tabCompletion": "on",
  "editor.wordWrap": "on",
  "editor.wordWrapColumn": 88,
  "editor.hover.enabled": false,
  "explorer.confirmDelete": false,
  "explorer.decorations.badges": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    ".babelrc": "jsonc",
    ".eslintrc": "jsonc",
    "*.pyi": "python"
  },
  "files.autoSave": "onFocusChange",
  "files.exclude": {
    "**/.DS_Store": true,
    "**/__pycache__": true,
    "**/.cache": true,
    "**/.idea": true,
    "**/.pytest_cache": true,
    "**/.mypy_cache": true,
    "**/.venv": true,
    "**/.coverage": true,
    "**/*.py[co]": true
  },
  "git.autofetch": true,
  "python.formatting.provider": "none",
  "python.pythonPath": "${workspaceFolder}/.venv/bin/python3",
  "python.terminal.activateEnvironment": true,
  "python.workspaceSymbols.enabled": true,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.enableBell": true,
  "window.clickThroughInactive": false,
  "window.newWindowDimensions": "maximized",
  "window.zoomLevel": -2,
  "workbench.colorCustomizations": {
    "activityBar.foreground": "#797d7b",
    "statusBar.background": "#1c1c1b",
    "gitDecoration.modifiedResourceForeground": "#d4cb68",
    "gitDecoration.addedResourceForeground": "#C2D94C",
    "editorSuggestWidget.highlightForeground": "#e5b358",
    "editorSuggestWidget.selectedBackground": "#1c2a42",
    "tab.inactiveForeground": "#676b6a",
    "sideBar.foreground": "#b3b1adBB",
  },
  "workbench.editor.labelFormat": "short",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.statusBar.feedback.visible": false,
  "workbench.tree.renderIndentGuides": "none",
  "workbench.tips.enabled": false,
  "python.linting.enabled": false,
  "git.showInlineOpenFileAction": false,
  "typescript.updateImportsOnFileMove.enabled": "never",
  "javascript.suggest.autoImports": false,
  "typescript.suggest.autoImports": false,
  "vetur.completion.autoImport": false,
  "vetur.experimental.templateInterpolationService": true,
  "breadcrumbs.enabled": true,
  "terminal.integrated.rendererType": "dom",
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.maxMemoryForLargeFilesMB": 10000,
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "go.useLanguageServer": true,
  "workbench.editor.showTabs": true,
}
